    "@types/react": "^18.2.52",
    "@types/react-dom": "^18.2.18",
    "@types/sanitize-html": "^2.9.5",
    "autoprefixer": "10.4.15",
    "dotenv": "^16.4.1",
    "eslint": "8.49.0",
    "eslint-config-next": "13.5.1",
    "knex": "^3.1.0",
    "next": "^14.1.0",
    "next-auth": "^4.24.5",
